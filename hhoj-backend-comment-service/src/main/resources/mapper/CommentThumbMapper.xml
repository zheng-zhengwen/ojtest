        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hh.hhojbackendcommentservice.mapper.CommentThumbMapper">

    <resultMap id="BaseResultMap" type="com.hh.hhojbackendmodel.entity.CommentThumb">
            <id property="id" column="id" jdbcType="BIGINT"/>
